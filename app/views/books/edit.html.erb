
<h1>Books#edit</h1>
<p>Find me in app/views/books/edit.html.erb</p>

<%= form_for(:book, :url => {:action => 'update', :id => @book.id}) do |f| %>
<%= f.text_field (:Name) %>
<%= f.text_field (:Subject) %>
<%= f.text_field (:Quantity) %>
<%= f.text_field (:Class) %>
<%= submit_tag("Edit Subject") %>
<% end %>